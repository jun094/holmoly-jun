{
    "compilerOptions": {
        "target": "es6",
        "lib": ["dom", "es6"],
        "jsx": "react",
        "module": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "include": [
        "./src/**/*",
        "./gatsby-node.ts",
        "./gatsby-config.ts",
        "./plugins/**/*"
    ]
}
